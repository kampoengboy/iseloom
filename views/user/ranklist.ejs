<div class="container" style="background-color:white">
  <br>
    <legend>Ranklists</legend>
    <div class="row">
      <div class="col-md-4">
        <div class="btn-group" role="group" aria-label="...">
          <button type="button" class="btn btn-default">User ranklist</button>
          <button type="button" class="btn btn-default">University ranklist</button>
        </div>
      </div>
      <div class="col-md-3">
        <select class="form-control">
          <option value="" disabled selected hidden>Ranklist for university</option>
        </select>
      </div>
    </div>
    <br>
    <br>
    <div class="table-responsive">
        <table class="table table-bordered table-hover table-striped">
          <thead>
                  <th>#</th>
                  <th>User</th>
                  <th>University</th>
                  <th>Grade</th>
                  <th>Rating</th>
          </thead>
          <tbody>
              <% var idx = 1 %>
              <% _.each(users, function(user){%>
                <tr>
                    <td><%=idx%></td>
                    <td><%=user.username%></td>
                    <td><%=user.university%></td>
                    <td><%=user.grade%></td>
                    <td><%=user.rating%></td>
                </tr>
                <% idx++ %>
              <%})%>

          </tbody>
        </table>
    </div>
</div>