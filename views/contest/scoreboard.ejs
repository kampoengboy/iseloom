<div class="container" style="background-color:white">
    <h3 class="text-center">Scoreboard <%=contest.name%></h3>
    <h4 id="clock" class="text-center"></h4>
    <table class="table table-bordered table-hover table-striped">
        <thead>
            <th>#</th>
            <th>Univ</th>
            <th>Name</th>
            <% _.each(problems, function(problem) { %>
                <th><a href="/problem/preview?prob=<%=problem.valName%>"><%=problem.valName%></a></th>
            <% }) %>
        </thead>
        <tbody>
            <% if(users.length > 0) { %>
           <% var idx = 1 %>
                  <% _.each(users, function(user){%>
                      <tr>
                          <td><%=idx%></td>
                          <td><%=user.id_user.university%></td>
                          <td><%=user.id_user.name%></td>
                          <% _.each(problems, function(problem) { %>
                            <td></td>
                          <% }) %>
                      </tr>
                      <% idx++ %>
                  <%})%>
            <% } %>
        </tbody>
    </table>
</div>
<script>
    var d = new Date('<%=contest.datetimeclose%>');
    var clock_div_name = '#clock';
    $(clock_div_name).countdown(d).on('update.countdown', function(event) {
        $(this).html(event.strftime('%D days %H:%M:%S'));
     }).on('finish.countdown', function(event) {
      $(this).html('Bid has closed!!!')
    });
</script>