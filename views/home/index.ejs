                    <p>This is the suggested problems</p>
                </div>

                <!-- Table -->
                <table class="table table-striped table-hover">
                    <thead>
                        <th>#</th>
                        <th>Problems</th>
                        <th>Solved</th>
                    </thead>
                    <tbody>
                        <tr>
                            <td>1</td>
                            <td>2048</td>
                            <td>112</td>
                        </tr>
